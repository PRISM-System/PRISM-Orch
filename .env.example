# E2E script client base URL (오케스트레이터 호출용)
APP_BASE_URL=http://localhost:8100

# Orch 서버 바인딩
APP_HOST=0.0.0.0
APP_PORT=8100
RELOAD=true

# vLLM(OpenAI-Compat) 서버 주소
OPENAI_BASE_URL=http://localhost:8001/v1
VLLM_MODEL=Qwen/Qwen3-14B
OPENAI_API_KEY=EMPTY

# PRISM-Core API 주소 (Orch → Core 호출용, llm-agent api 주소)
PRISM_CORE_BASE_URL=http://localhost:8000

# GitHub Token for private repos (if needed)
GITHUB_TOKEN=your_github_token_here

# Vector Database Configuration
VECTOR_ENCODER_MODEL=sentence-transformers/paraphrase-multilingual-mpnet-base-v2
VECTOR_DIM=768
WEAVIATE_PORT=18080
WEAVIATE_API_KEY=
WEAVIATE_API_KEY_ENABLED=false

# Class Prefix for Weaviate classes
CLASS_PREFIX=KOSHA

# Processing Configuration
MAX_DOCUMENTS=

# PRISM-Core Version Control
# Local Development (default)
USE_LOCAL_PRISM_CORE=true

# Production/Testing with GitHub (uncomment to use)
#USE_LOCAL_PRISM_CORE=false
#PRISM_CORE_VERSION=main

# Available PRISM_CORE_VERSION options:
# - main: Latest development version
# - v0.2.8: Specific release tag
# - v0.2.9: Newer release tag  
# - feature-branch-name: Specific feature branch

# Platform API Endpoints
PLATFORM_API_ENDPOINT=
MONITORING_API_ENDPOINT=
PREDICTION_API_ENDPOINT=localhost:1100
AUTOCONTROL_API_ENDPOINT=localhost:1200